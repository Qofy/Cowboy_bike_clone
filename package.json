{
	"name": "cowboy-bike-clone",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"json-server": "json-server --watch data/accessories.json --port 3001",
		"server": "json-server --watch data/service.json --port 3002",
		"server1": "json-server --watch data/bike_data.json --port 3003",

		"dev:all": "concurrently \"npm run dev\" \"npm run json-server\" \"npm run server\" \"npm run server1\""
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.36.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@types/node": "^22",
		"concurrently": "^9.2.1",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"json-server": "^1.0.0-beta.3",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.39.5",
		"vite": "^7.1.7"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.17",
		"tailwindcss": "^4.1.17"
	}
}